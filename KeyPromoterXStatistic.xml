<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="New Folder...">
        <Item>
          <IdeaActionID>FileChooser.NewFolder</IdeaActionID>
          <ShortCut>'Alt+Insert' or 'Ctrl+N'</ShortCut>
          <Description>New Folder...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Quick Switch Scheme...">
        <Item>
          <IdeaActionID>QuickChangeScheme</IdeaActionID>
          <ShortCut>'Ctrl+`'</ShortCut>
          <Description>Quick Switch Scheme...</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'Alt+6'</ShortCut>
          <Description>TODO</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Unit Tests">
        <Item>
          <IdeaActionID>ActivateUnitTestsToolWindow</IdeaActionID>
          <ShortCut>'Alt+8' or 'Alt+Shift+8'</ShortCut>
          <Description>Unit Tests</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9' or 'Alt+Shift+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>